<h1>New Character Form</h1>
<div>
<%= form_for @person, html: { multipart: true } do |f| %>
</div>
<div>
<%= f.label :name %>
<%= f.text_field :name %>
</div>

<div>
<%= f.label :description %>
<%= f.text_field :description, :cols => "10", :rows => "10" %>
</div>

<div>
<%= f.label :image %>
<%= f.file_field :image %>
</div>
<%= f.submit "Add Character"  %>

<% end %>
