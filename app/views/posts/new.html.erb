<h1>New Post</h1>
<div>
<%= form_for @post, html: { multipart: true } do |f| %>
</div>
<div>
<%= f.label :title %>
<%= f.text_field :title %>
</div>

<div>
<%= f.label :description %>
<%= f.text_field :description%>
</div>

<div>
<%= f.label :image %>
<%= f.file_field :image %>
</div>
<%= f.submit "Add Post"  %>

<% end %>
